{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [2, { "forbid": ["array", "object"] }]
  },
  "parser": "babel-eslint",
  "globals": { "fetch": false },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true,
    "jest": true,
    "amd": true,
    "browser": true
  }
}
